{
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 2, {
      "SwitchCase": 1
    }],
    "max-len": [1, 124, 4],
    "camelcase": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/forbid-prop-types": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off"
  },
  "globals": {
    "alert": true,
    "console": true,
    "document": true,
    "localStorage": true,
    "it": true,
    "navigator": true,
    "window": true
  }
}
